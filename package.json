  "author": "Powerise Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "firebase-tools": "^14.9.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@firebasegen/default-connector": "link:dataconnect-generated/js/default-connector"
  }
